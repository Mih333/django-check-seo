"\n"
"%(nbproblems)s problem found\n"
msgid_plural ""
"\n"
"%(nbproblems)s problems found\n"
msgstr[0] ""
"\n"
"%(nbproblems)s problème trouvé\n"
msgstr[1] ""
"\n"
"%(nbproblems)s problèmes trouvés\n"

#: django-check-seo/templates/default.html:33
#, python-format
#| msgid ""
#| "\n"
#| "          , and %(warnings)s warning raised.\n"
#| "        "
#| msgid_plural ""
#| "\n"
#| "          , and %(warnings)s warnings raised.\n"
#| "        "
msgid ""
"\n"
", and %(nbwarnings)s warning raised.\n"
msgid_plural ""
"\n"
", and %(nbwarnings)s warnings raised.\n"
msgstr[0] ""
"\n"
", et %(nbwarnings)s avertissement levé.\n"
msgstr[1] ""
